{
    "name": "novactive/ezedithelpbundle",
    "description": "Add a helper when editing or creating a Content. This helper allows you to display personalised text for the Content creation/edition",
    "keywords": [
        "tooltip",
        "ibexa",
        "ibexa5",
        "novactive",
        "almaviacx"
    ],
    "homepage": "https://github.com/Novactive/NovaeZEditHelpBundle",
    "type": "ibexa-bundle",
    "authors": [
        {
            "name": "AlmaviaCX",
            "homepage": "https://almaviacx.com/expertises/web-mobile/",
            "email": "dirtech.web@almaviacx.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.3",
        "ibexa/oss": "^5",
        "guzzlehttp/promises": "^2.3",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Novactive\\Bundle\\NovaeZEditHelpBundle\\": "bundle/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "ibexa/post-install": true
        }
    }
}
